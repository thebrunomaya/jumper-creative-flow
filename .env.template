# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Service role key (needed for admin operations and migrations)
# WARNING: Keep this secret! Only use in server-side code
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Notion Integration (for creative publishing)
NOTION_TOKEN=your-notion-integration-token
NOTION_DB_CRIATIVOS_ID=your-notion-database-id
NOTION_DB_CONTAS_ID=your-notion-accounts-database-id
NOTION_DB_PARCEIROS_ID=your-notion-partners-database-id